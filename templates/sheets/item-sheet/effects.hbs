<section data-tab="{{tabs.effects.id}}" class="{{tabs.effects.cssClass}}" data-group="{{tabs.effects.group}}">
  <button type="button" data-action="createEffect" class="icon fa-solid fa-plus"></button>
  {{#if effects.enabledEffects.length}}
  {{effects-element documents=effects.enabledEffects action="renderEmbedded" inline=false label=(localize "RYUUTAMA.ITEM.activeEffects")}}
  {{/if}}

  {{#if effects.disabledEffects.length}}
  {{effects-element documents=effects.disabledEffects action="renderEmbedded" inline=false label=(localize "RYUUTAMA.ITEM.disabledEffects")}}
  {{/if}}
</section>
